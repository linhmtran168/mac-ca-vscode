{
  "name": "mac-ca-vscode",
  "displayName": "Mac CA VSCode",
  "description": "Make all certificates in Keychain Access available to VS Code extensions",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.0.0"
  },
  "publisher": "linhmtran168",
  "author": {
    "name": "Linh M. Tran",
    "email": "linh.mtran168@live.com"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linhmtran168/mac-ca-vscode"
  },
  "bugs": {
    "url": "https://github.com/linhmtran168/mac-ca-vscode"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "@vscode/test-electron": "^2.1.3"
  }
}
